  "private": true,
  "author": "Nashvillest <tips@nashvillest.com>",
  "description": "Honoring the legacy of Nashville public access star Joey Bowker.",
  "dependencies": {
    "@stephenyeargin/hubot-openweathermap": "^1.1.2",
    "hubot": "^4",
    "hubot-airnow-gov": "^1.1.5",
    "hubot-bcycle-finder": "^2.3.1",
    "hubot-dad-jokes": "^1.1.2",
    "hubot-diagnostics": "1.2.1",
    "hubot-discord": "^2.1.0",
    "hubot-dotenv": "^1.0.0",
    "hubot-flickr-group-search": "^1.1.21",
    "hubot-google-images": "^0.2.7",
    "hubot-help": "^2.1.2",
    "hubot-hockey": "^6.1.1",
    "hubot-homepage": "^1.1.3",
    "hubot-nashville-electric": "^1.2.1",
    "hubot-nashville-news": "^1.1.2",
    "hubot-plusplus": "^1.4.0",
    "hubot-pollen": "^1.2.1",
    "hubot-redis-brain": "hubotio/hubot-redis-brain",
